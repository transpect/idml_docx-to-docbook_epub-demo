<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://www.le-tex.de/resource/schema/hub/1.1/css/cssa-rules.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<css:rules xmlns:css="http://www.w3.org/1996/css"
           comment="  2013-11-26T18:02:38.192+01:00">
   <css:rule name="Blocktext"
             native-name="Block Text"
             layout-type="para"
             css:border-bottom-color="#4F81BD"
             css:border-bottom-style="solid"
             css:border-bottom-width="0.1pt"
             css:border-left-color="#4F81BD"
             css:border-left-style="solid"
             css:border-left-width="0.1pt"
             css:border-right-color="#4F81BD"
             css:border-right-style="solid"
             css:border-right-width="0.1pt"
             css:border-top-color="#4F81BD"
             css:border-top-style="solid"
             css:border-top-width="0.1pt"
             css:color="#4F81BD"
             css:font-style="italic"
             css:margin-left="57.6pt"
             css:margin-right="57.6pt"/>
   <css:rule name="Textkrper"
             native-name="Body Text"
             layout-type="para"
             css:margin-bottom="6pt"/>
   <css:rule name="Textkrper2"
             native-name="Body Text 2"
             layout-type="para"
             css:line-height="24pt"
             css:margin-bottom="6pt"/>
   <css:rule name="Textkrper3"
             native-name="Body Text 3"
             layout-type="para"
             css:font-size="8pt"
             css:margin-bottom="6pt"/>
   <css:rule name="Textkrper-Einzug2"
             native-name="Body Text Indent 2"
             layout-type="para"
             css:line-height="24pt"
             css:margin-bottom="6pt"
             css:margin-left="14.15pt"/>
   <css:rule name="Textkrper-Einzug3"
             native-name="Body Text Indent 3"
             layout-type="para"
             css:font-size="8pt"
             css:margin-bottom="6pt"
             css:margin-left="14.15pt"/>
   <css:rule name="Datum" native-name="Date" layout-type="para"/>
   <css:rule name="Aufzhlungszeichen"
             native-name="List Bullet"
             layout-type="para"/>
   <css:rule name="Aufzhlungszeichen2"
             native-name="List Bullet 2"
             layout-type="para"/>
   <css:rule name="Aufzhlungszeichen3"
             native-name="List Bullet 3"
             layout-type="para"/>
   <css:rule name="Aufzhlungszeichen4"
             native-name="List Bullet 4"
             layout-type="para"/>
   <css:rule name="Aufzhlungszeichen5"
             native-name="List Bullet 5"
             layout-type="para"/>
   <css:rule name="Listennummer"
             native-name="List Number"
             layout-type="para"/>
   <css:rule name="Listennummer2"
             native-name="List Number 2"
             layout-type="para"/>
   <css:rule name="Listennummer3"
             native-name="List Number 3"
             layout-type="para"/>
   <css:rule name="Listennummer4"
             native-name="List Number 4"
             layout-type="para"/>
   <css:rule name="Listennummer5"
             native-name="List Number 5"
             layout-type="para"/>
   <css:rule name="Listenabsatz"
             native-name="List Paragraph"
             layout-type="para"
             css:margin-left="36pt"/>
   <css:rule name="Standardeinzug"
             native-name="Normal Indent"
             layout-type="para"
             css:margin-left="35.4pt"/>
   <css:rule name="Zitat"
             native-name="Quote"
             layout-type="para"
             css:color="#000000"
             css:font-style="italic"/>
   <css:rule name="Untertitel"
             native-name="Subtitle"
             layout-type="para"
             css:color="#4F81BD"
             css:font-size="12pt"
             css:font-style="italic"/>
   <css:rule name="Titel"
             native-name="Title"
             layout-type="para"
             css:border-bottom-color="#4F81BD"
             css:border-bottom-style="solid"
             css:border-bottom-width="0.4pt"
             css:color="#17365D"
             css:font-size="26pt"
             css:line-height="12pt"
             css:margin-bottom="15pt"/>
   <css:rule name="Beschriftung"
             native-name="caption"
             layout-type="para"
             css:color="#4F81BD"
             css:font-size="9pt"
             css:font-weight="bold"
             css:line-height="12pt"/>
   <css:rule name="berschrift1"
             native-name="heading 1"
             layout-type="para"
             css:color="#365F91"
             css:font-size="14pt"
             css:font-weight="bold"
             css:margin-bottom="0pt"
             css:margin-top="24pt"
             remap="h1"/>
   <css:rule name="berschrift2"
             native-name="heading 2"
             layout-type="para"
             css:color="#4F81BD"
             css:font-size="13pt"
             css:font-weight="bold"
             css:margin-bottom="0pt"
             css:margin-top="10pt"
             remap="h2"/>
   <css:rule name="berschrift3"
             native-name="heading 3"
             layout-type="para"
             css:color="#4F81BD"
             css:font-weight="bold"
             css:margin-bottom="0pt"
             css:margin-top="10pt"
             remap="h3"/>
   <css:rule name="berschrift4"
             native-name="heading 4"
             layout-type="para"
             css:color="#4F81BD"
             css:font-style="italic"
             css:font-weight="bold"
             css:margin-bottom="0pt"
             css:margin-top="10pt"
             remap="h4"/>
   <css:rule name="berschrift5"
             native-name="heading 5"
             layout-type="para"
             css:color="#243F60"
             css:margin-bottom="0pt"
             css:margin-top="10pt"
             remap="h5"/>
   <css:rule name="berschrift6"
             native-name="heading 6"
             layout-type="para"
             css:color="#243F60"
             css:font-style="italic"
             css:margin-bottom="0pt"
             css:margin-top="10pt"
             remap="h6"/>
   <css:rule name="berschrift7"
             native-name="heading 7"
             layout-type="para"
             css:color="#404040"
             css:font-style="italic"
             css:margin-bottom="0pt"
             css:margin-top="10pt"
             remap="h7"/>
   <css:rule name="berschrift8"
             native-name="heading 8"
             layout-type="para"
             css:color="#404040"
             css:font-size="10pt"
             css:margin-bottom="0pt"
             css:margin-top="10pt"
             remap="h8"/>
   <css:rule name="berschrift9"
             native-name="heading 9"
             layout-type="para"
             css:color="#404040"
             css:font-size="10pt"
             css:font-style="italic"
             css:margin-bottom="0pt"
             css:margin-top="10pt"
             remap="h9"/>
   <css:rule name="Buchtitel"
             native-name="Book Title"
             layout-type="inline"
             css:font-variant="small-caps"
             css:font-weight="bold"/>
   <css:rule name="Hyperlink"
             native-name="Hyperlink"
             layout-type="inline"
             css:color="#0000FF"
             css:text-decoration-line="underline"/>
   <css:rule name="Fett"
             native-name="Strong"
             layout-type="inline"
             css:font-weight="bold"/>
   <css:rule name="Funotenzeichen"
             native-name="footnote reference"
             layout-type="inline"
             remap="superscript"/>
   <css:rule name="Tabellenraster"
             native-name="Table Grid"
             layout-type="table"
             css:line-height="12pt"
             css:margin-bottom="0pt"/>
  <css:rule name="Funotentext"
             native-name="Footnote Text"
             layout-type="para"
             css:line-height="0pt"
             css:margin-bottom="0pt"/>
  <css:rule  name="hub:lists"/>
  <css:rule  name="hub:identifier"/>
  <css:rule  name="hub:caption-text"/>
  <css:rule  name="hub:caption-number"/>
  <css:rule  name="endnote"/>
  <css:rule  name="Endnotenzeichen"/>
  <css:rule  name="Endnotentext"/>
</css:rules>